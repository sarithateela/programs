import java.math.BigInteger;
class Main {
    public static void main(String[] args) {
       System.out.println(fabinocci(5));
    }
    public static int[] fabinocci(int n)
    {
        if(n<=0) return new int[0];
        
      int[] s1=new int[n];
      s1[0]=0;
      if(n>1)s1[1]=1;
      //s1[1]=1;
       for(int i=2;i<n;i++)
       {
           s1[i]=s1[i-1]+s1[i-2];
       }
       
       
       
       
       return s1;
    }
}
