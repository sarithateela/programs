
class Solution {
    ArrayList<Integer> nFibonacci(int N) {
        // code here
       ArrayList<Integer> result = new ArrayList<>();
       int a=0,b=1;
       result.add(a);
       if(N==0) return result;
       while(b<=N)
       {
           result.add(b);
           int next=a+b;
           a=b;
           b=next;
       }
       return result;
    }
}
